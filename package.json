{
  "name": "jsxbin",
  "version": "2.0.1",
  "description": "Convert jsx ExtendScript files into jsxbin files using ExtendScript Toolkit",
  "keywords": [
    "after effects",
    "extendscript",
    "illustrator",
    "indesign",
    "jsxbin",
    "photoshop"
  ],
  "bugs": "https://github.com/runegan/jsxbin/issues",
  "license": "MIT",
  "author": "Rune Gangs√∏ <runegan@gmail.com> (http://runegan.no)",
  "files": [
    "index.js",
    "command.js",
    "src/*",
    "esdebugger-core.tar.gz",
    "esdebugger-core-win.tar.gz"
  ],
  "main": "index.js",
  "bin": {
    "jsxbin": "./command.js"
  },
  "repository": "github:runegan/jsxbin",
  "scripts": {
    "test": "eslint . && mocha",
    "postinstall:default": "tar -xzf esdebugger-core.tar.gz",
    "postinstall:win32": "tar -xzf esdebugger-core-win.tar.gz",
    "postinstall": "./node_modules/.bin/run-script-os"
  },
  "dependencies": {
    "command-line-args": "^4.0.4",
    "command-line-usage": "^4.0.0",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "run-script-os": "^1.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-runegan": "^1.0.1",
    "mocha": "^6.2.1",
    "mz": "^2.6.0",
    "rimraf": "^2.6.1"
  },
  "os": [
    "darwin",
    "win32"
  ]
}
